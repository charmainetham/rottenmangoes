

<h1> Welcome to the Admin Page</h1>
<p><%= link_to "Create new user here!",new_admin_user_path %>
<hr>
<%= paginate @users %>
<% @users.each do |user| %>
  <li><h3><%= user.full_name %></h3>
    <p><% if user.admin? %>(admin) <% else %>(regular)</p><% end %>
    <p><%= user.email%></p>
    <%= button_to "Delete", admin_user_path(user.id), :method => :delete %>
    <p><%= link_to "edit user", edit_admin_user_path(user) %></p>
  </li>
<% end %>
<hr>
<%= link_to "Back to movie page", movies_path %> 
<hr>
